<p id="notice"><%= notice %></p>

<h1>Tos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tos.each do |to| %>
      <tr>
        <td><%= to.name %></td>
        <td><%= link_to 'Show', to %></td>
        <td><%= link_to 'Edit', edit_to_path(to) %></td>
        <td><%= link_to 'Destroy', to, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New To', new_to_path %>
